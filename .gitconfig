[push]
	defult = upstream
[merge]
	conflictstyle = diff3
[init]
  defaultBranch = main
[alias]
	adog = log --abbrev-commit --all --decorate --oneline --graph --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' 
